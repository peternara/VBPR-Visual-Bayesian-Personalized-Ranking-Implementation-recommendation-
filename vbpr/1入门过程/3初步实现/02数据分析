 if row['asin'] == 0 :\n","            return '00'\n","        if row['asin'] == 1 :\n","            return '01'\n","        if row['asin'] == 2 :\n","            return '02'\n","        if row['asin'] == 3 :\n","            return '03'\n","        if row['asin'] == 4 :\n","            return '04'\n","        if row['asin'] == 5 :\n","            return '05'\n","        if row['asin'] == 6 :\n","            return '06'\n","        if row['asin'] == 7 :\n","            return '07'\n","        if row['asin'] == 8 :\n","            return '08'\n","        if row['asin'] == 9 :\n","            return '09'\n","        if row['asin'] == 10 :\n","            return '10'\n","        if row['asin'] > 10 :\n","            return '>10' "],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"2OKFHySAIAwp","colab_type":"code","colab":{}},"source":["df2['flag']=df2.apply(lambda row: flag(row),axis=1)\n","df3 = df2.groupby(['flag']).reviewerID.sum().reset_index()\n","df3['pct'] = df3['reviewerID']/(df3['reviewerID'].sum())*100"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"-khZJWyMD5x1","colab_type":"code","outputId":"448f4804-2a12-453c-a9a9-5b31946e880e","executionInfo":{"status":"ok","timestamp":1558186058956,"user_tz":-480,"elapsed":1306,"user":{"displayName":"Jacob Ji","photoUrl":"https://lh3.googleusercontent.com/-zoRekIA1fCY/AAAAAAAAAAI/AAAAAAAACBQ/eV12vlBRQiA/s64/photo.jpg","userId":"06638368627777389358"}},"colab":{"base_uri":"https://localhost:8080/","height":306}},"source":["ax = df3.sort_values('flag',ascending=True).plot(kind='bar',x = 'flag',y='pct', color ='#80cbc4',title='%customers by number of reviews' ,legend = False,edgecolor='black')\n","ax.set_xlabel('# reviews')\n","ax.set_ylabel('% customers')\n","ax.set_facecolor('#fffbf0')\n","plt.show()\n","\n","## This graph says that 40% of the customers write 5 reviews \n","## 75% of the reviews are written by customers writing upto 7 reviews"],"execution_count":0,"outputs"