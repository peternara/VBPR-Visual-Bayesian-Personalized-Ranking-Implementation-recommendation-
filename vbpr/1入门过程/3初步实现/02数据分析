lpful = df_review['helpful']\n","yes=[]\n","no = []\n","for i in range(len(df_review['helpful'])):\n","  x = df_review['helpful'][i][0]\n","  yes.append(x)\n","  y = df_review['helpful'][i][1]\n","  no.append(y)\n","  "],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"pOUoO4LDuTQ1","colab_type":"code","colab":{}},"source":["df_review['helpful_yes'] = yes\n","df_review['helpful_no'] = no\n"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"06dvhplQuonC","colab_type":"code","colab":{}},"source":["helpful_review = df_review[['overall','helpful_yes','helpful_no']].groupby('overall').sum().reset_index()\n","\n","helpful_review['yes_pct'] = helpful_review['helpful_yes']*100/(helpful_review['helpful_yes']+ helpful_review['helpful_no'])\n","helpful_review['no_pct'] = helpful_review['helpful_no']*100/(helpful_review['helpful_yes']+ helpful_review['helpful_no'])"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"ZxAyI56cvSVt","colab_type":"code","outputId":"e34f966f-b189-4ac6-a123-6e4deea1aa9d","executionInfo":{"status":"ok","timestamp":1554991710888,"user_tz":-480,"elapsed":3166,"user":{"displayName":"Jacob Ji","photoUrl":"https://lh3.googleusercontent.com/-zoRekIA1fCY/AAAAAAAAAAI/AAAAAAAACBQ/eV12vlBRQiA/s64/photo.jpg","userId":"06638368627777389358"}},"colab":{"base_uri":"https://localhost:8080/","height":318}},"source":["ax = helpful_review[['overall','yes_pct','no_pct']].plot(kind = 'bar', x = 'overall', title = 'product helpfulness'\\\n","                                                        ,color =['#00695c','#af4345'])\n","                                                        \n","ax.set_xlabel('Overall Ratings')\n","ax.set_ylabel('% Helpful - Yes/No ')\n","ax.legend(('%Yes','%No'),loc=4)"],"execution_count":0,"outputs":[{"output_type":"execute_result","data":{"text/plain":["<matplotlib.legend.Legend at 0x7fbb291dd9b0>"]},"metadata":{"tags":[]},"execution_count":15},{"output_type":"display_data"